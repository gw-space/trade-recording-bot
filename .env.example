GOOGLE_SERVICE_ACCOUNT_FILE=/Users/test/AIassistant/service_account.json
TELEGRAM_BOT_TOKEN=your_bot_token

# Drive API 없이 실행하려면 종목:스프레드시트ID 매핑을 반드시 설정
# 예: TQQQ:1AbCdEf...,BTC:1XyZ...
SPREADSHEET_ID_MAP=TQQQ:your_tqqq_sheet_id,BTC:your_btc_sheet_id
# 또는 JSON 파일 매핑 사용 (파일이 있으면 해당 값 우선 적용)
# 예: {"TQQQ":"id1","BTC":"id2","SOXL":"id3"}
SPREADSHEET_ID_MAP_FILE=
FALLBACK_OPEN_BY_TITLE=false
WORKSHEET_NAME=

TIMEZONE=Asia/Seoul
STATE_FILE=/Users/test/AIassistant/state.json
START_FROM_LATEST_ON_FIRST_RUN=true
TELEGRAM_POLL_TIMEOUT=30
TELEGRAM_POLL_INTERVAL=2

# Upbit auto fill -> bitcoin sheet
UPBIT_ENABLED=true
UPBIT_ACCESS_KEY=your_upbit_access_key
UPBIT_SECRET_KEY=your_upbit_secret_key
UPBIT_BASE_URL=https://api.upbit.com
# 다중 코인 매핑 (권장): <업비트마켓>:<시트심볼>
# 예) KRW-BTC:BTC,KRW-ETH:ETH
UPBIT_MARKET_SHEET_MAP=KRW-BTC:BTC
# 업비트 명령 접두어 (기본: "업비트")
UPBIT_COMMAND_PREFIX=업비트
# (레거시 단일 설정 - MAP 비어 있을 때만 사용)
UPBIT_MARKET=KRW-BTC
UPBIT_SHEET_SYMBOL=BTC
UPBIT_ORDERS_PATH=/v1/orders
SPREADSHEET_BACKUP_DIR=/Users/test/AIassistant/spreadsheet_backups
